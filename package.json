{
  "name": "lantoo",
  "description": "Rocket.Chat Language Tool",
  "version": "0.0.9",
  "author": "Allan Juan @ajsaraujo",
  "bin": {
    "lantoo": "./bin/run"
  },
  "bugs": "https://github.com/ajsaraujo/lantoo/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "@types/inquirer": "^7.3.3",
    "fs-extra": "^10.0.0",
    "fuse.js": "^6.4.6",
    "inquirer": "^8.1.2",
    "reflect-metadata": "^0.1.13",
    "tslib": "^1.14.1",
    "tsyringe": "^4.5.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@rocket.chat/eslint-config": "^0.4.0",
    "@types/chai": "^4.2.18",
    "@types/chai-as-promised": "^7.1.4",
    "@types/fs-extra": "^9.0.11",
    "@types/mocha": "^5.2.7",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.30.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "fancy-test": "^1.4.10",
    "globby": "^10.0.2",
    "husky": "^6.0.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.1",
    "ts-mocha": "^8.0.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ajsaraujo/lantoo",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "lantoo",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": [
        "./lib/hooks/init"
      ]
    }
  },
  "repository": "ajsaraujo/lantoo",
  "scripts": {
    "pretty-quick": "pretty-quick",
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "ts-mocha \"src/**/*.spec.ts\" --paths -p tsconfig.json --bail",
    "coverage": "nyc --extension .ts mocha --forbid-only \"src/**/*.spec.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "lint": "eslint src/**/*.ts --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "types": "lib/index.d.ts"
}
